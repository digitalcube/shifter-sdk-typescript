{
  "version": "0.4.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "generate-sdk": "./.bin/generate-sdk.sh",
    "docs": "typedoc src/index.ts"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@shifter/sdk",
  "author": "Digitalcube",
  "module": "dist/shifter-sdk-node.esm.js",
  "size-limit": [
    {
      "path": "dist/shifter-sdk-node.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/shifter-sdk-node.esm.js",
      "limit": "10 KB"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/request": "^2.48.5",
    "husky": "^6.0.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digitalcube/experimental-shifter-sdk-typescript.git"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "es6-promise": "^4.2.4",
    "url-parse": "^1.5.9",
    "whatwg-fetch": "^3.0.0"
  }
}
